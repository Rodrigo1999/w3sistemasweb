{
  "name": "cxviz-eventloop",
  "version": "1.0.1",
  "description": "Event Loop diagrams for StrongTrace visualization",
  "main": "index.js",
  "scripts": {
    "test": "tap test/test-*.js",
    "demo": "rm -f mdemo/build.js && ./node_modules/browserify/bin/cmd.js demo/script.js -o demo/build.js && ./node_modules/serve/bin/serve demo",
    "watch": "./node_modules/watchify/bin/cmd.js demo/script.js -o demo/build.js --debug"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/strongloop/cxviz-eventloop.git"
  },
  "keywords": [
    "strongloop",
    "diagram",
    "d3",
    "waterfall",
    "event loop"
  ],
  "homepage": "https://github.com/strongloop/cxviz-eventloop",
  "license": {
    "name": "StrongLoop",
    "url": "https://github.com/strongloop/cxviz-eventloop/blob/master/LICENSE"
  },
  "author": "StrongLoop <engineering@strongloop.com>",
  "bugs": {
    "url": "https://github.com/strongloop/cxviz-eventloop/issues"
  },
  "dependencies": {
    "component-emitter": "^1.1.2",
    "strong-trace-waterfalltransform": "^1.0.0",
    "cxviz-color": "1.0.0",
    "d3": "^3.4.6",
    "jquery": "^2.1.1"
  },
  "devDependencies": {
    "browserify": "^4.1.2",
    "strong-trace-waterfalldata": "~1.0.0",
    "serve": "^1.4.0",
    "tap": "~0.7.0",
    "watchify": "^0.10.2"
  },
  "jshintConfig": {
    "node": true,
    "browser": true,
    "devel": true,
    "jquery": true,
    "undef": true,
    "unused": true,
    "asi": true,
    "indent": 2,
    "eqnull": true,
    "loopfunc": true,
    "eqeqeq": false,
    "newcap": false,
    "globalstrict": true,
    "validthis": true
  }
}
